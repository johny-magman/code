package com.example.myapplication;

public class DB_class {
    private String name;
    private String carbs;
    private String portion;

    public DB_class (){}
    public DB_class(String name, String carbs, String portion){
        this.name=name;
        this.carbs=carbs;
        this.portion=portion;

    }
    public void setName(String y){
        this.name=y;

    }
    public void setPortion(String z){
        this.portion=z;

    }
    public void setCarbs(String x){
        this.carbs=x;

    }



    public String getName(){
        return this.name;
    }

    public String getCarbs(){
        return this.carbs;
    }

    public String getPortion(){
        return this.portion;
    }
}
